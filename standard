abs(x) = if x > 0 then x else -x;
turn(x) = -x;
max(a, b) = if a > b then a else b;
min(a, b) = -max (-a) (-b);
iwanttobelieve = print "HAIL OUR SAVIOR, THE FLYING SPAGHETTI MONSTER!!!";
exit() = print "I want you to stay here.";
abs(x) = if x > 0 then x else -x;
fac(x) = if x < 2 then x else x * fac (x - 1);
empty(l) = l == [];
length(x:xs) = if empty xs then 1 else 1 + length xs;
sum(x:xs) = if empty xs then x else x + sum xs;
map(f, xs) = f x for x in xs;